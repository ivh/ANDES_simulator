#!/usr/bin/env python3
"""
ANDES Simulation Framework CLI Entry Point

Provides the 'andes-sim' command for unified ANDES E2E simulations.
"""

import sys
from pathlib import Path

# Add the andes_simulator package to Python path
sys.path.insert(0, str(Path(__file__).parent))

from andes_simulator.cli.main import main

if __name__ == '__main__':
    main()